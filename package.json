{
  "name": "@choiceform/expression-engine",
  "version": "0.1.1",
  "description": "High-performance, secure frontend expression engine compatible",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
